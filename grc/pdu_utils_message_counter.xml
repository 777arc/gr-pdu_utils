<?xml version="1.0"?>
<block>
  <name>Message Counter</name>
  <key>pdu_utils_message_counter</key>
  <category>[Sandia]/PDU Utilities/Debug Utils</category>
  <import>import pmt</import>
  <import>import pdu_utils</import>
  <make>pdu_utils.message_counter(pmt.intern($name))</make>

  <param>
    <name>Name</name>
    <key>name</key>
    <type>string</type>
  </param>

  <sink>
    <name>msg</name>
    <type>message</type>
  </sink>

</block>
