<?xml version="1.0"?>
<block>
  <name>Time Delta</name>
  <key>pdu_utils_time_delta</key>
  <category>[Sandia]/PDU Utilities</category>
  <import>import pdu_utils</import>
  <make>pdu_utils.time_delta($name)</make>

  <param>
    <name>Name</name>
    <key>name</key>
    <value>'Time Printer'</value>
    <type>string</type>
  </param>

  <sink>
    <name>pdu_in</name>
    <type>message</type>
  </sink>

  <doc>Block will attempt to access the 'wall_clock_time' PDU dictionary element and print the time delta.</doc>

</block>
